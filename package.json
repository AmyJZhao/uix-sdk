{
  "name": "@adobe/uix-sdk-monorepo",
  "private": true,
  "version": "0.1.0",
  "description": "Monorepo for @adobe/uix-sdk development",
  "scripts": {
    "any": "npm run --workspaces --if-present",
    "build": "npm run any build",
    "clean": "rm -rf dist .parcel-cache node_modules/.cache examples/*/node_modules/.cache && npm run any clean",
    "demo": "npm run any example:build && node scripts/multi-server.mjs demo",
    "dev": "node scripts/multi-server.mjs dev",
    "docs": "npm run run any docs",
    "format": "prettier --write --loglevel warn packages/*/src package.json packages/*/package.json",
    "lint": "prettier --check packages/*/src && eslint packages/*/src",
    "test": "npm run any test",
    "watch": "run-s dev"
  },
  "repository": {
    "type": "git",
    "url": "https://git.corp.adobe.com/dx-devex-acceleration/uix-sdk.git"
  },
  "author": "Adobe, Inc,",
  "license": "UNLICENSED",
  "devDependencies": {
    "@adobe/react-spectrum": "^3.19.0",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@microsoft/api-documenter": "^7.19.0",
    "@microsoft/api-extractor": "^7.28.6",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-react": "^2.0.0",
    "chalk": "^5.0.1",
    "concurrently": "^7.2.2",
    "esbuild": "^0.14.50",
    "eslint": "^8.19.0",
    "figures": "^4.0.1",
    "humanize-duration": "^3.27.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.6.2",
    "penpal": "~6.2.2",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17",
    "serve": "^14.0.1",
    "tsc-watch": "^5.0.3",
    "typescript": "~4.6.3",
    "vite": "^3.0.3",
    "zalgo-js": "^1.0.0"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ]
}
