{
  "name": "@adobe/uix-host-react",
  "version": "0.6.2",
  "description": "Tools for embedding UI Extensions into React apps",
  "author": "Adobe, Inc,",
  "type": "module",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent"
  },
  "browserslist": [
    "> 0.2%, last 2 versions, not dead"
  ],
  "bugs": "https://git.corp.adobe.com/dx-devex-acceleration/uix-sdk/issues",
  "dependencies": {
    "@adobe/uix-core": "^0.6.2",
    "@adobe/uix-host": "^0.6.2",
    "penpal": "~6.2.2"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "tsconfig.json"
  ],
  "homepage": "https://git.corp.adobe.com/dx-devex-acceleration/uix-sdk",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://git.corp.adobe.com/dx-devex-acceleration/uix-sdk.git"
  },
  "sideEffects": false,
  "source": {
    ".": "./src/index.ts",
    "./*.js": "./src/$1.ts",
    "./**/*.js": "./src/$1/$2.ts"
  }
}
