{
  "name": "@adobe/uix-host-react",
  "version": "0.4.0",
  "description": "Tools for embedding UI Extensions into React apps",
  "type": "module",
  "browser": "dist/index.js",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*.js": {
      "import": "./dist/$1.js",
      "require": "./dist/$1.cjs"
    },
    "./**/*.js": {
      "import": "./src/$1/$2.js",
      "require": "./src/$1/$2.cjs"
    }
  },
  "source": {
    ".": "./src/index.ts",
    "./*.js": "./src/$1.ts",
    "./**/*.js": "./src/$1/$2.ts"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "run-p compile:*",
    "clean": "rm -rf dist *.tsbuildinfo docs/temp",
    "compile:bundle": "tsup",
    "compile:emit": "tsc --build tsconfig.json",
    "debug-build": "tsc --project tsconfig-debug.json --verbose",
    "watch": "run-p watch:*",
    "watch:bundle": "tsup --watch --silent",
    "watch:emit": "tsc --watch"
  },
  "browserslist": [
    "> 0.2%, last 2 versions, not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.corp.adobe.com/dx-devex-acceleration/uix-sdk.git"
  },
  "author": "Adobe, Inc,",
  "license": "UNLICENSED",
  "dependencies": {
    "@adobe/uix-core": "^0.4.0",
    "@adobe/uix-host": "^0.4.0",
    "penpal": "~6.2.2"
  }
}